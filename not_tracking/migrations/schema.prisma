datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Project {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  link        String
  createdAt   DateTime @default(now())
}

model Resume {
  id          Int      @id @default(autoincrement())
  name        String
  experience  String
  education   String
  certifications String
}
